      "source": "chatgpt",
      "title": "documentTitle",
      "conversation": "//title",
      "message": "//div[@data-message-author-role]",
      "selectorType": "semantic",
      "inManifest": true,
      "inUrlDetector": true,
      "notes": "Stable; works well."
    },
    "chatgpt.com": {
      "source": "chatgpt",
      "title": "documentTitle",
