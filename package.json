{
    "name": "ai-saver-chrome-extension",
    "version": "0.1.0",
    "description": "Chrome extension to automatically save AI conversations from ChatGPT, Claude, Perplexity, and Kimi",
    "type": "module",
    "scripts": {
      "dev": "npm run dev --prefix chrome_extension",
      "build": "npm run build --prefix chrome_extension",
      "preview": "vite preview",
      "icons": "node scripts/generate-icons.js",
      "test": "playwright test",
      "test:ui": "playwright test --ui",
      "test:headed": "playwright test --headed",
      "test:debug": "playwright test --debug",
      "validate:selectors": "tsx scripts/validate-extractor-selectors.ts"
    },
    "keywords": [
      "chrome-extension",
      "ai",
      "chatgpt",
      "claude",
      "conversation-saver"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
      "@playwright/test": "^1.40.0",
      "@types/chrome": "^0.0.268",
      "@types/node": "^20.10.6",
      "typescript": "^5.3.3",
      "vite": "^5.0.11",
      "vite-plugin-web-extension": "^4.1.1",
      "tsx": "^4.7.0"
    },
    "dependencies": {}
  }